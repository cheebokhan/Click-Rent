body {
    font-family: sans-serif;
    background: #f6f8f9;
    padding: 0px;
    margin: 0px;

  }
  footer {
    font-size: 9px;
    color: #f00;
    text-align: center;
  }
  @media print {
    footer {
      position: fixed;
      bottom: 0;
      left: 0;
      height: 2em;
      font-size: small;
    }
    .content-block {
      page-break-inside: avoid;
      margin-top: "20px";
      margin-bottom: "20px";
    }
    p {
      page-break-inside: avoid;
    }
  }
  @page {
    margin:10mm 10mm 10mm 10mm
  }
  
  .no-break {
    break-inside: avoid;
  }

  tfoot{
    display: table-row-group;
  }

  
  

  